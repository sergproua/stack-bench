{
  "name": "health-claims-backend",
  "version": "0.1.0",
  "private": true,
  "description": "NestJS backend for MongoDB performance demo",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "node -r @swc-node/register src/main.ts",
    "build": "npm run typecheck && swc src -d dist --source-maps",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "seed": "tsx scripts/seed.ts",
    "seed:generate": "tsx scripts/seed-generate.ts",
    "seed:load": "tsx scripts/seed-load.ts",
    "indexes": "tsx scripts/indexes.ts",
    "bench": "tsx scripts/benchmark.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/websockets": "^10.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.1",
    "mongodb": "^6.4.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@swc-node/register": "^1.10.4",
    "@swc/cli": "^0.3.11",
    "@swc/core": "^1.4.17",
    "@types/node": "^20.11.17",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
